{
  "name": "NevermoreIntegrationTest",
  "globIgnorePaths": [
    "**/.package-lock.json",
    "**/.pnpm",
    "**/.pnpm-workspace-state-v1.json",
    "**/.modules.yaml",
    "**/.ignored",
    "**/.ignored_*"
  ],
  "tree": {
    "$className": "DataModel",
    "ServerScriptService": {
      "integration": {
        "$className": "Folder",
        "game": {
          "$path": "modules"
        },
        "node_modules": {
          "$path": "node_modules"
        }
      },
      "Script": {
        "$path": "scripts/Server"
      }
    },
    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Main": {
          "$path": "scripts/Client"
        }
      }
    }
  }
}