{
  "name": "{{gameNameProper}}",
  "globIgnorePaths": [
    "**/.package-lock.json",
    "**/.pnpm",
    "**/.pnpm-workspace-state-v1.json",
    "**/.modules.yaml",
    "**/.ignored",
    "**/.ignored_*"
  ],
  "tree": {
    "$className": "DataModel",
    "ServerScriptService": {
      "{{gameNameProper}}": {
        "$className": "Folder",
        "game": {
          "$path": "src/modules"
        },
        "node_modules": {
          "$path": "node_modules"
        }
      },
      "{{gameNameProper}}Scripts": {
        "$path": "src/scripts/Server"
      }
    },
    "StarterPlayer": {
      "StarterPlayerScripts": {
        "{{gameNameProper}}Scripts": {
          "$path": "src/scripts/Client"
        }
      }
    }
  }
}