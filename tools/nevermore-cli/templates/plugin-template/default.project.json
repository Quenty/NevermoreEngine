{
  "name": "{{pluginNameProper}}",
  "globIgnorePaths": [
    "**/.package-lock.json",
    "**/.pnpm",
    "**/.pnpm-workspace-state-v1.json",
    "**/.modules.yaml",
    "**/.ignored",
    "**/.ignored_*"
  ],
  "tree": {
    "$path": "src",
    "node_modules": {
      "$className": "Folder",
      "$path": "node_modules"
    }
  }
}