{
  "name": "{{packageNameProper}}Test",
  "globIgnorePaths": [
    "**/.package-lock.json",
    "**/.pnpm",
    "**/.pnpm-workspace-state-v1.json",
    "**/.modules.yaml",
    "**/.ignored",
    "**/.ignored_*"
  ],
  "tree": {
    "$className": "DataModel",
    "ServerScriptService": {
      "{{packageName}}": {
        "$path": ".."
      },
      "Script": {
        "$path": "scripts/Server"
      }
    },
    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Main": {
          "$path": "scripts/Client"
        }
      }
    }
  }
}